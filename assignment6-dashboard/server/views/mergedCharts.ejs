     axisX: {
                    title:"Employment Sectors", 
                    //valueFormatString: "YYYY",
                    crosshair: {
                        enabled: true,
                        snapToDataPoint: true
                    }
                },
                axisY: {
                    title: "Percentage",
                    gridThickness: 1,
                    gridColor: "black",
                    crosshair: {
                        enabled: true,
                        snapToDataPoint: true
                    }
                },
    legend: {
        // horizontalAlign: "left",
        verticalAlign: "bottom",
          fontSize: 15
              },
    data: [{
        type: "bar",
        indexLabelFontSize: 16,
        indexLabelFontColor:"white",
        startAngle: 95,
        //showInLegend: true,
        // toolTipContent: "{label}:</b>{y}%",

        indexLabel: "{y}%",
        indexLabelFontSize:25,
        indexLabelPlacement: "inside", 
        dataPoints: []
    }]
 });
let percentageData = '<%- JSON.stringify(percentage) %>';
            let obj3 = JSON.parse(percentageData);
            Object.keys(obj3).forEach(function (key) {
                chart.options.data[0].dataPoints.push(
                    { label: JSON.stringify(obj3[key].sector), y: obj3[key].percentage,legendText:JSON.stringify(obj3[key].sector)}
                );
            });
chart.render();

            CanvasJS.addColorSet("earlyShades",
                [//colorSet Array

                "#a7ce9f",
                "#5e9a78",
                "#365253",
                "#ce8732",
                "#f7d98a",
                "#ac9760"               
                ]);

        var chart = new CanvasJS.Chart("chartContainer4",
        {
            colorSet: "earlyShades",

                {
                    type: "line",
                    showInLegend: true,
                    axisYType: "primary",
                    //color: "rgb(204, 163, 0)",
                    name: "Total Number of Startups - California",
                    showInLegend: true,
                    markerSize: 10,
                    dataPoints: []
                }
                ]
            });

            let womenData = '<%- JSON.stringify(womenData) %>';
            let obj2 = JSON.parse(womenData);
            Object.keys(obj2).forEach(function (key) {
                chart.options.data[0].dataPoints.push(
